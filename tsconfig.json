{
    "compilerOptions": {
        "alwaysStrict": true,
        "target": "ES2017",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": false,
        "experimentalDecorators": true,
        "strictNullChecks": true,
        "pretty": true,
        "lib": [
            "dom",
            "es5",
            "es2015",
            "es2016"
        ],
        "types": ["node"]
    }
}
